<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.ssh_block.script</string>

  <key>ProgramArguments</key>
  <array>
    <string>/etc/sshd_fwscan.sh</string>
  </array>

  <key>Nice</key>
  <integer>1</integer>

  <key>StartInterval</key>
  <integer>60</integer>

  <key>RunAtLoad</key>
  <true/>

  <key>StandardErrorPath</key>
  <string>/tmp/ssh_block.err</string>

  <key>StandardOutPath</key>
  <string>/tmp/ssh_block.out</string>
</dict>
</plist>
